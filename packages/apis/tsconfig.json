{
    "compilerOptions": {
        "lib": ["es6", "esnext.asynciterable"],
        "skipLibCheck": true,
        "module": "esnext",
        "allowJs": false,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "noEmit": false,
        "emitDeclarationOnly": true,
        "declaration": true,
        "rootDir": "./src",
        "outDir": "."
    },
    "include": ["src/**/*"],
    "exclude": ["./*.d.ts"]
}
