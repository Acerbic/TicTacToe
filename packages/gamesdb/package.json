{
  "name": "@trulyacerbic/ttt-gamesdb",
  "version": "1.0.27",
  "description": "Prisma graphql database model definition",
  "main": "generated/prisma-ts-client",
  "private": true,
  "scripts": {
    "build": "yarn generate && tsc",
    "prisma": "prisma",
    "generate": "prisma generate",
    "deploy": "prisma generate && prisma deploy",
    "clean": "rimraf generated",
    "hasura:apply": "hasura migrate apply --project hasura && hasura metadata apply --project hasura",
    "hasura:getgraphql": "gq http://localhost:8080/v1/graphql --introspect > hasura.graphql"
  },
  "keywords": [
    "prisma",
    "tictactoe",
    "game",
    "graphql",
    "database"
  ],
  "author": "Varenov Gleb",
  "license": "ISC",
  "dependencies": {
    "graphqurl": "^0.3.3",
    "hasura-cli": "^1.2.1",
    "prisma-client-lib": "^1.34.10"
  },
  "devDependencies": {
    "prisma": "^1.34.10",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.2"
  }
}
