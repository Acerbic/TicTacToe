{
  "name": "@trulyacerbic/ttt-gamesdb",
  "version": "1.1.0",
  "description": "Hasura graphql database meta and model bindings",
  "private": true,
  "scripts": {
    "hasura": "hasura --project hasura",
    "deploy": "hasura migrate apply --project hasura && hasura metadata apply --project hasura",
    "console": "hasura console --project hasura",
    "generate": "graphql-inspector introspect http://localhost:8080/v1/graphql --write hasura.graphql"
  },
  "keywords": [
    "hasura",
    "tictactoe",
    "game",
    "graphql",
    "database"
  ],
  "author": "Varenov Gleb",
  "license": "ISC",
  "dependencies": {
    "@graphql-inspector/cli": "^2.3.0",
    "graphql": "^15.4.0",
    "hasura-cli": "^1.2.1",
    "rimraf": "^3.0.2"
  }
}
