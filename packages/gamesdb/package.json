{
  "name": "@trulyacerbic/ttt-gamesdb",
  "version": "1.1.0",
  "description": "Hasura graphql database meta and model bindings",
  "private": true,
  "scripts": {
    "hasura": "hasura --project hasura",
    "deploy": "hasura migrate apply --project hasura && hasura metadata apply --project hasura",
    "console": "hasura console --project hasura",
    "generate": "gq http://localhost:8080/v1/graphql --introspect > hasura.graphql"
  },
  "keywords": [
    "hasura",
    "tictactoe",
    "game",
    "graphql",
    "database"
  ],
  "author": "Varenov Gleb",
  "license": "ISC",
  "dependencies": {
    "graphql": "^0.13.1",
    "graphqurl": "^0.3.3",
    "hasura-cli": "^1.2.1",
    "rimraf": "^3.0.2"
  }
}
